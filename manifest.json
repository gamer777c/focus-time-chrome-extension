{
  "manifest_version": 3,
  "name": "Productivity Tracker",
  "version": "1.2",
  "description": "Track time spent on websites and analyze productivity.",
  "permissions": ["tabs", "storage", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
